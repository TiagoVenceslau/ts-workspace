{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "node_modules/jsdoc-mermaid",
        "node_modules/better-docs/typescript",
        "node_modules/better-docs/category"
    ],
    "typescript": {
        "moduleRoot": "src"
    },
    "package": "package.json",
    "recurseDepth": 10,
    "source": {
        "include": [
            "./src"
        ],
        "exclude": [],
        "includePattern": "\\.(jsx|js|ts|tsx)$"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "better-docs": {
            "name": "ts-template",
            "title": "TypeScript workspace template",
            "hideGenerator": true,
            "navLinks": [
                {
                    "label": "Github Repository",
                    "href": "https://github.com/TiagoVenceslau/ts-workspace#readme"
                },
                {
                    "label": "Github Profile",
                    "href": "https://github.com/TiagoVenceslau"
                }
            ]
        },
        "cleverLinks": true,
        "monospaceLinks": true,
        "search": true
    },
    "opts": {
        "encoding": "utf-8",
        "template": "./node_modules/better-docs",
        "destination": "./docs/",
        "readme": "./README.md",
        "recurse": true,
        "verbose": true,
        "tutorials": "workdocs/tutorials"
    }
}